#include<stdio.h>
int main()
{
    int a[3],b[3],i,s=0,t=0;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
            s=s+1;
        }
        else if(a[i]<b[i])
        {
            t=t+1;
        }
    }
    printf("%d %d",s,t);
}
