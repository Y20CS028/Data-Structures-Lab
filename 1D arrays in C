#include <stdio.h>
#include <string.h>
#include <math.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,i,s=0;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        s+=a[i];
    }   
    printf("%d",s);
    return 0;
}
